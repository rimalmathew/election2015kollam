<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.pranavshenoy.election.contacts"
    >

<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_weight="1">
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/sec"
        android:padding="10dp"
        android:background="@drawable/sec_button"
        android:layout_below="@+id/emt"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
        <!--android:layout_alignParentTop="true"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_alignParentEnd="true" />-->

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/da"
        android:padding="10dp"
        android:background="@drawable/da_button"
        android:layout_alignBaseline="@+id/pokr"
        android:layout_alignBottom="@+id/pokr"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_alignParentEnd="true" />-->
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/dew"
        android:padding="10dp"
        android:background="@drawable/dew_button"
        android:layout_below="@+id/sec"
        android:layout_alignRight="@+id/da"
        android:layout_alignEnd="@+id/da" />
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_alignParentEnd="true" />-->

</LinearLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_weight="1">

        <Button
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/pokr"
            android:padding="10dp"
            android:background="@drawable/pokr_button"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_weight="1">


        <Button

        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/poc"
        android:padding="10dp"
        android:background="@drawable/pokc_button"
        android:layout_above="@+id/brp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignParentStart="true" />-->



    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/brp"
        android:padding="10dp"
        android:background="@drawable/brp_button"
        android:layout_alignBaseline="@+id/dew"
        android:layout_alignBottom="@+id/dew"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/emt"
        android:padding="10dp"
        android:background="@drawable/emt"
        android:layout_alignTop="@+id/da"
        android:layout_centerHorizontal="true" />
    <!--android:layout_alignParentBottom="true"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignParentStart="true"-->
         <!-- -->


   </LinearLayout>

</LinearLayout>
